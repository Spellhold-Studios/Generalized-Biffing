/* ===================================== *
 *  Biff only .wav, .tis and .bam files  *
 * ===================================== */
BEGIN @10
DESIGNATED 0 LABEL "generalized_biffing_min"
SUBCOMPONENT @1

OUTER_SET biffing_all = 0
INCLUDE ~%MOD_FOLDER%/lib/main_component.tpa~

/* ================ *
 *  Biff all files  *
 * ================ */
BEGIN @11
DESIGNATED 1 LABEL "generalized_biffing_all_files"
SUBCOMPONENT @1

OUTER_SET biffing_all = 1
INCLUDE ~%MOD_FOLDER%/lib/main_component.tpa~
